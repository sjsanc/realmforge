{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "lint": "eslint src/**/*.ts",
    "start": "npm run lint && npm run build && npm run dev",
    "test": "npm run build && ava",
    "dev": "node ./dist/src/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.2",
    "@types/node": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "fs-extra": "^8.1.0",
    "typescript": "~3.7.0",
    "ava": "^3.8.2",
    "@ava/typescript": "^1.1.1"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "timeout": "1m",
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  }
}
